def weatherName(weather3, weather4):
    print(weather3,weather4)

weather = input("Enter a seasson name : ")
weather2 = input("Enter a seasson name : ")
weatherName(weather, weather2)

def season_Name(mon):
    if mon == "Octber":
        print("winter")
    elif mon == "April":
        print("Summer")
month = input("Enter a month name")
season_Name(month)

